{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "DocumentFragment": true
    },
    "rules": {
        "valid-jsdoc": 0,
        "curly": [1, "multi"],
        "brace-style": [2, "stroustrup"],
        "consistent-this": 2,
        "no-underscore-dangle": 0,
        "func-names": 0,
        "func-style": [2, "declaration"],
        "new-cap": 2,
        "new-parens": 2,
        "no-ternary": 0,
        "quotes": [2, "single"],
        "one-var": 0,
        "strict": 0,
        "no-unused-vars": 1
    }
}
